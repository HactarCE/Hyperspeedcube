a = 3

fn export_stuff() {
    export a
    export b = 4
    c = 5
    export c
    export fn d() { return 15 }
    return // ok because no value
}

m = export_stuff()
assert_eq(m.a, 3)
assert_eq(m.b, 4)
assert_eq(m.c, 5)
assert_eq(m.d(), 15)

fn dubious() {
    export a
    return 10
}
assert_eq("return statement after export", __eval_to_error(dubious))

export q = 10
export r = 15
export s = 20
