a = 3

fn export_stuff() {
    export a
    export b = 4
    c = 5
    export c
    export fn d() { return 15 }
    return // ok because no value
}

m = export_stuff()
assert_eq(m.a, 3)
assert_eq(m.b, 4)
assert_eq(m.c, 5)
assert_eq(m.d(), 15)

fn dubious() {
    export a
    return 10
}
assert_eq("return statement after export", __eval_to_error(dubious))

export q = 10
r = 15
export r
export fn s() { 20 }
export * from #{ t: 25, u: 30 }
export v, x as xx from #{ v: 35, w: 40, x: 45 }
export (y, z) from #{ y: 50, z: 55 }
