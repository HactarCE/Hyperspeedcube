l = [0, 10, 20, 30, 40]

assert_eq(l[0], 0)
assert_eq(l[1], 10)
assert_eq(l[2], 20)
assert_eq(l[3], 30)
assert_eq(l[4], 40)
assert_eq(l[5], null)
assert_eq(l[100], null)

assert_eq(l[-1], 40)
assert_eq(l[-2], 30)
assert_eq(l[-3], 20)
assert_eq(l[-4], 10)
assert_eq(l[-5], 0)
assert_eq(l[-6], null)
assert_eq(l[-100], null)
